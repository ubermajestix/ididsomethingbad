<h1>New backlog</h1>

<%= error_messages_for :backlog %>

<% form_for(@backlog) do |f| %>
<p>
  <b>Title</b><br />
  <%= f.text_field :title %>
</p>

  <p>
    <b>Feature</b><br />
    <%= f.text_area :feature, :size=>"60x2" %>
  </p>



  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', backlogs_path %>
