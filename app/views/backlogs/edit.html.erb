<h1>Editing backlog</h1>

<%= error_messages_for :backlog %>

<% form_for(@backlog) do |f| %>
  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <b>Feature</b><br />
    <%= f.text_area :feature %>
  </p>

  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @backlog %> |
<%= link_to 'Back', backlogs_path %>
